{
    "name": "lianhechx/laravel-modules",
    "description": "Laravel Modules Package",
    "keywords": ["laravel", "modules"],
    "license": "MIT",
    "authors": [
        {
            "name": "Chen",
            "email": "lianhechx@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.6.4",
        "illuminate/database": "5.3.*",
        "illuminate/support": "5.3.*",
        "illuminate/filesystem": "5.3.*",
        "illuminate/config": "5.3.*",
        "illuminate/routing": "5.3.*"
    },
    "autoload": {
        "psr-4": {
            "Opbol\\Modules\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        }
    },
    "suggest": {
        "wikimedia/composer-merge-plugin": "Allows the ability to create and merge composer.json files for your individual modules for module-specific dependency management."
    }
}
